@model List<SubtitlesSiteMVCFrontend.Views.ViewModels.UploaderStats>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Upload Count</th>
            <th>Latest Upload</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Username</td>
                <td>@item.UploadCount</td>
                <td>@item.LatestUpload.ToString("yyyy-MM-dd")</td>
            </tr>
        }
    </tbody>
</table>